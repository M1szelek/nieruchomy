<li>
  <div class="offer">
       <div class="offertitle"><br /> <%= offer.title %><br /> </div>
      
       
       <div class="offerphoto">
       	<!--<%= offer.photo %> -->
       	</br>
       	
       	<%= image_tag "home.jpg" %>
       	</br>
       </div>
        
        <div class="offerdetails">
        	
		
		<b>Cena:</b>
		<%= offer.price %> zł
		<br />
		<b>Typ:</b>
		 <%= offer.typeM %>
		<br />		
		<b>Liczba pokoi:</b>
		<%= offer.roomcount %>
		<br />	
		<b>Metraż:</b>
		<%= offer.metrage %> m^2
		<br />
		<b>Województwo:</b>
		<%= offer.state %>
			<br />	
		<b>Miasto:</b>
		<%= offer.city %>
			<br />	
		<b>Ulica:</b>
		<%= offer.street %>
		<br />
		<b>Uużytkownik</b>
		<%= offer.user_id %>
		<br />
			</div>
        <div class="offerD">
        	<b>Opis:</b>
        	<br />
        	  <%= offer.description %>
        </div>
        
     
    </div>
  <span class="timestamp">
    Posted <%= time_ago_in_words(offer.created_at) %> ago.
  </span>
  <% if current_user?(offer.user) %>
    <%= link_to "delete", offer, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: offer.title %>
  <% end %>
</li>